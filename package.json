{
  "name": "cph-leetcode",
  "displayName": "cph-leetcode",
  "description": "CPH LeetCode Test Cases",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "leetcode-testcases",
          "title": "LeetCode Test Cases",
          "icon": "media/leetcode.svg"
        }
      ]
    },
    "views": {
      "leetcode-testcases": [
        {
          "type": "webview",
          "id": "leetcode-testcases.webview",
          "name": "CPH Test Cases"
        }
      ]
    },
    "commands": [
      {
        "command": "leetcode-testcases.fetch",
        "title": "CPH: Fetch Test Cases"
      },
      {
        "command": "leetcode-testcases.run",
        "title": "CPH: Run Test Cases"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "path": "^0.12.7"
  }
}
